{
    "require": {
        "google/apiclient": "^2.11",
        "monolog/monolog": "^2.3",
        "sabre/xml": "^2.2",
        "symfony/console": "^5.3"
    },
    "bin": ["bin/console"],
    "autoload": {
         "psr-4": {
             "Console\\": "src"
         },
         "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },

    "require-dev": {
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpunit/phpunit": "^9.5",
        "symfony/test-pack": "^1.0",
        "clue/phar-composer": "^1.2"
    }
}
